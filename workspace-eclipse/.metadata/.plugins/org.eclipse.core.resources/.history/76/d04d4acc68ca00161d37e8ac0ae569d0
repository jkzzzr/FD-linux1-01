package test;

import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.FileWriter;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.OutputStream;
import java.util.zip.GZIPInputStream;

public class displayLength {

	public static void main(String[] args) throws Exception {
		GZIPInputStream gzipInputStream = new GZIPInputStream(new FileInputStream("/media/clueweb09_1of2/ClueWeb09_English_1/en0000/00.warc.gz"));
		BufferedWriter bWriter = new BufferedWriter(new FileWriter(""));
		BufferedReader bReader = new BufferedReader(new InputStreamReader(gzipInputStream, "UTF-8"));
		long index = 0L;
		while (true){	
			String temp = bReader.readLine();
			if (temp == null)break;
			index +=temp.length()+1;
			bWriter.write("["+index+"]\t"+temp+"\t");
		}
		bWriter.flush();
		bWriter.close();
	}

}
